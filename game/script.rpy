

label start:
    show screen black_screen
    with dissolve
    pause 1.0
    hide screen black_screen
    with dissolve
    jump prolog_pertama


label prolog_pertama:
    scene background:
        "#e5faffff"
    "[text_prolog[1]]" with dissolve
    "[text_prolog[2]]" with dissolve
    "[text_prolog[3]]" with dissolve
    "[text_prolog[4]]" with dissolve
    
